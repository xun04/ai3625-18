prefix=/data2/ai3625/workspace/ai3625-18/miniconda3
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include
supported_protocols="DICT FILE FTP FTPS GOPHER GOPHERS HTTP HTTPS IMAP IMAPS IPFS IPNS MQTT POP3 POP3S RTSP SCP SFTP SMB SMBS SMTP SMTPS TELNET TFTP WS WSS"
supported_features="alt-svc AsynchDNS HSTS HTTP2 HTTPS-proxy IDN IPv6 Largefile libz NTLM SSL threadsafe TLS-SRP UnixSockets zstd"

Name: libcurl
URL: https://curl.se/
Description: Library to transfer files with HTTP, FTP, etc.
Version: 8.16.0
Requires: 
Requires.private: libssh2,libidn2,openssl,zlib,libzstd,libnghttp2
Libs: -L${libdir} -lcurl 
Libs.private: -lpthread -lssh2 -lidn2 -lssl -lcrypto -lz -lzstd -lnghttp2
Cflags: -I${includedir} 
Cflags.private: -DCURL_STATICLIB
